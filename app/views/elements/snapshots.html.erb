<% content_for :breadcrumbs do %>
	<div id="breadcrumbs">
		<ul class="breadcrumb">
			<li><i class="icon-home"></i><a href="<%= root_path %>">Home</a><span class="divider"><i class="icon-angle-right"></i></span></li>
			<li class="active">Snapshots</li>
		</ul>
	</div>
<% end %>
<% content_for :header do %>
	<div class="page-header position-relative">
		<h1>Snapshots</h1>
	</div><!--/page-header-->
<% end %>

<div class="row-fluid">
	<div class="span12" style="margin-left:0px;" id="list-snapshots">
		<!-- Content to be loaded using Ajax -->
	</div><!--/span-->
</div><!--/row-->

<script type='text/javascript'>
	window.onload = function() {
		$('li#nav-elements').addClass('active open');
		$('li#nav-element-snapshots').addClass('active');
		return $list_snapshots();
	};
</script>